{
  "name": "@vibe-validate/extractors",
  "version": "0.18.2",
  "description": "LLM-optimized error extractors for validation output",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/test/helpers/extractor-test-helpers.d.ts",
      "import": "./dist/test/helpers/extractor-test-helpers.js"
    }
  },
  "files": [
    "dist",
    "error-extractor-result.schema.json"
  ],
  "scripts": {
    "build": "tsc && node dist/scripts/generate-result-schema.js",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:report": "tsx test/generate-quality-report.ts",
    "test:regression": "tsx test/generate-quality-report.ts --check"
  },
  "keywords": [
    "validation",
    "error-extraction",
    "llm-friendly",
    "typescript",
    "eslint",
    "vitest",
    "openapi"
  ],
  "author": "Jeff Dutton",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdutton/vibe-validate.git",
    "directory": "packages/extractors"
  },
  "devDependencies": {
    "@types/node": "^20.19.26",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "yaml": "^2.8.2"
  },
  "dependencies": {
    "@vibe-validate/config": "workspace:*",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.25.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
